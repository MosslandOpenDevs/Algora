{
  "name": "@algora/governance-os",
  "version": "0.1.0",
  "description": "Unified Agentic Governance Operating System for Algora v2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@algora/core": "workspace:*",
    "@algora/safe-autonomy": "workspace:*",
    "@algora/orchestrator": "workspace:*",
    "@algora/document-registry": "workspace:*",
    "@algora/model-router": "workspace:*",
    "@algora/dual-house": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "algora",
    "governance",
    "agentic",
    "autonomous",
    "ai-governance",
    "dual-house",
    "orchestrator"
  ],
  "license": "MIT"
}
